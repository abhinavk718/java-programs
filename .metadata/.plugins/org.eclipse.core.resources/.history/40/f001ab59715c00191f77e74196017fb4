import java.util.*;
public class ZensarProgram 
{
	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the number");
		int num=sc.nextInt();
		int res=sum(num);
		System.out.println(res);
		sc.close();
	}
	public static int sum(int num)
	{
		int sum=0;
		while(num>0)
		{
			int r=num%10;
			sum=sum+r;
			num=num/10;
		}
		return sum;
	}
}
